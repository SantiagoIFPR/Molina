	<queryString>
		<![CDATA[SELECT
     produto."id" AS produto_id,
     produto."nome_produto" AS produto_nome_produto,
     produto."quantidade_estoque" AS produto_quantidade_estoque,
     produto."categoria_id" AS produto_categoria_id,
     categoria."nome" AS categoria_nome
FROM
     "public"."categoria" categoria INNER JOIN "public"."produto" produto ON categoria."id" = produto."categoria_id"
WHERE
     produto."quantidade_estoque" > 0
ORDER BY
     produto.nome_produto ASC]]>
